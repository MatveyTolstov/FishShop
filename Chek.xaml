<Window x:Class="gh.Chek"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gh"
        mc:Ignorable="d"
        Title="Chek" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.1*"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <ComboBox Name="Chek_ComboBox" Grid.Row="0" SelectionChanged="Chek_ComboBox_SelectionChanged" ></ComboBox>
        <DataGrid x:Name="ChekGr" Grid.Row="1"></DataGrid>
    </Grid>
</Window>
